{
  "name": "music-interface-toolkit",
  "private": true,
  "scripts": {
    "clean": "lerna exec -- git clean -Xdf -e node_modules",
    "dev": "trap 'exit 0' INT; lerna exec --scope @musedlab/piano-ui -- rollup -c -w",
    "postdev": "npm run clean",
    "build": "lerna exec -- rollup -c",
    "prepublishOnly": "npm run build",
    "postpublish": "npm run clean",
    "document": "eleventy --config=doc-gen/config.js --pathprefix=/music-interface-toolkit/",
    "document-serve": "eleventy --serve --config=doc-gen/config.js"
  },
  "author": "NYU Music Experience Design Lab (MusEDLab)",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.1",
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@wessberg/rollup-plugin-ts": "^1.2.34",
    "lerna": "^3.22.1",
    "rollup": "^1.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.9.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@musedlab/midi": "file:packages/midi",
    "@musedlab/midi-ui": "file:packages/midi-ui",
    "@musedlab/piano-ui": "file:packages/piano-ui",
    "@musedlab/toolkit": "file:packages/toolkit",
    "@types/react": "^16.9.43"
  }
}
